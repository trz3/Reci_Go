@page
@model Reci_Go.Pages.Difficulty.GetAllModel
@{
}
<table class="table table-striped table-dark">
	<tr>
		<th scope="col">#</th>
		<th scope="col">Name</th>
	</tr>
	@foreach (var user in Model.Difficulties)
	{
		<tr>
			<td scope="row">@user.Id</td>
			<td>@user.Name</td>
			<td>
				<a asp-page="/User/Update" asp-route-id="@user.Id">Update</a> | <!--Change delete to have user ID to a default one-->
				<a asp-page="/User/Delete" asp-route-id="@user.Id">Delete</a>

			</td>
		</tr>
	}
